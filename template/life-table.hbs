<h5>Life</h5>
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Class</th>
            <th>Description</th>
            <th>Life</th>
        </tr>
    </thead>
    <tbody>
    {{#each this}}
        <tr>
            <td>{{name}}</td>
            <td>{{className}}</td>
            <td>{{classification}}</td>
            <td>{{life.name}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>
